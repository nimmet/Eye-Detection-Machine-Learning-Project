   "source": [
    "# To change our images from PNG to JPG \n",
    "\n",
    "png = glob(r\"spot_data\\spot_images\\*.png\")\n",
    "\n",
    "for j in png:\n",
    "    print(j)\n",
    "    img = cv2.imread(j)\n",
    "    cv2.imwrite(j[:-3]+\"jpg\",img)\n",
    "    os.remove(j)"
   ]
